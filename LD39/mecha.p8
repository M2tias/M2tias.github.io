end

function get_flag(x, y, flag)
	local tile = mget(x/8, y/8)
	local flag = fget(tile, flag)
	return flag
end

function _draw()
	if(intro) then
		intro_draw()
	elseif(ending) then
		ending_draw()
	elseif(win) then
		win_draw()
	else
		game_draw()
	end
end

