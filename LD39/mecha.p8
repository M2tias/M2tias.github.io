char.speed.y = 0
char.flipx = false
char.jump = false
char.hp = 10
char.inv_time = 1.3
char.inv_timer = 0
char.invulnerable = false
char.shootframes = 0
char.frame = 0

cam = {}
cam.x = 64
cam.y = 64

backdrop = {}
backdrop.x = 0
backdrop.max_x = 55
backdrop.y = 48
backdrop.w = 32
backdrop.h = 8

mech = {}
mech.x = 0
mech.y = 0
mech.sprx = 8 --60*8
mech.spry = 11
mech.battery = 8
mech.hull = 10
mech.inv_time = 2
mech.inv_timer = 0
mech.invulnerable = false
mech.height = 16 -- todo: check this value
mech.frame = 0
mech.shooting = false

mech.frames = {
	fleg = {
		x = {0, -1, -1, 0, 1, 2, 2, 1},
		y = {0,  0, -1,-1,-1,-1, 0, 0}
	},
	bleg = {
		x = { 0, 1, 2, 2, 1, 0, -1, -1},
		y = {-1,-1,-1, 0, 1, 0,  0, -1}
	},
	gun = {
		x = {-1, -4, -4, -4, -3, -2, -1},
		frame = 0 -- if frame == #gun.x, we'll leave the animation
	}
}

bullets = {} -- {x, y, dir, damage}
rockets = {} -- {x, y, dx, dy, step, explode, damage}
enemies = {} -- {x, y, frame, maxframes, height, dir, animated, sprite, underground, health}
og_moving = false -- true means overground enemies can move
batteries = {} -- {x, y}


intro = true
intro_page = 0
last_page = 2
music_ = true
ending = false
end_sound = false
win = false
win_sound = false

frames1 = {0, 1, 2, 3, 4}
frames2 = {0, 1, 2, 3, 4}
frame = 0
step = 0
fstep = 0
debug_p = {x = 0, y = 0}
debug_r = {x1 = 0, y1 = 0, x2 = 0, y2 = 0}

function _init()
	for x = 0,127 do
		for y = 0,30 do
			--overground enemies
			local oflag = get_flag(x*8, y*8, 4)
			if(oflag) then
				add(enemies, {
					x = x*8,
					y = y*8,
					frame = 0,
					maxframes = 3,
					height = 2,
					dir = false,
					animated = true,
					sprite = 74,
					underground = false,
					health = 4
				})
				add(enemies, {
					x = x*8+4,
					y = y*8,
					frame = 0,
					maxframes = 3,
					height = 2,
					dir = false,
					animated = true,
					sprite = 74,
					underground = false,
					health = 4
				})
				mset(x, y, 0)
			end

			--underground enemies
			local uflag = get_flag(x*8, y*8, 5)
			if(uflag) then
				--create enemy
				add(enemies, {
					x = x*8,
					y = y*8,
					frame = 0,
					maxframes = 3,
					height = 1,
					dir = false,
					animated = true,
					sprite = 106,
					underground = true,
					health = 1
				})
				mset(x, y, 0)
			end

			--batteries
			local bflag = get_flag(x*8, y*8, 2)
			if(bflag) then
				add(batteries, {
					x = x*8,
					y = y*8
				})
				mset(x, y, 0)
			end
		end
	end
end

function _update()
	local current_t = time()
	if(not music_) then 
		music_ = true
		music(0)
	end
	--mech.battery = mech.battery - 0.05
	if(intro) then
		--create_ground_enemies()
		if(btnp() ~= 0) then
			if(intro_page == last_page) then
				intro = false
			else 
				intro_page = intro_page + 1
			end
		end
	elseif(ending) then
		if(not end_sound) then
			sfx(7)
			end_sound = true
		end
	elseif(win) then
		if(not win_sound) then
			sfx(10)
			win_sound = true
		end
	else
		mech_update(current_t)
		char_update(current_t)
		other_update()
		bullet_update()
	end
end

function mech_update(current_t)
	if(mech.battery > 0 and char.inside) then
		if(og_moving) then
			mech.sprx = mech.sprx + 0.33
		else
			mech.sprx = mech.sprx + 0.9
		end
		if(btnp(4, 0)) then
			mech_shoot()
		elseif(btnp(5, 0)) then
			--create_underground_enemy()
			mech_rockets()
		end
	elseif(char.inside) then
		char.x = mech.sprx+5
		char.y = mech.spry
		char.speed.y = -3
		char.inside = false
		sfx(8)
	end

	if(current_t - mech.inv_timer >= mech.inv_time) then
		mech.invulnerable = false
	end

	if(mech.hull <= 0) then
		ending = true
		end_text = "mecha has been destroyed."
	end

	if(mech.sprx > 8*101 and char.inside) then
		mech.battery = 0
		--char.inside = false
	end
end

function char_update(current_t)
	if(char.inside) then return end -- character doesn't interact inside mech
	
	if(btn(0, 0)) then
		char.speed.x = -1
		char.flipx = true
	elseif(btn(1, 0)) then
		char.speed.x = 1
		char.flipx = false
	else
		char.speed.x = 0
	end
	
	if(btn(2, 0)) then
		char_up()
		char.aiming = false
	elseif(btn(3, 0)) then
		char_down()
	else
		if(char.speed.y < 0) then
			--char.speed.y = char.speed.y + 1
		else
			--char.speed.y = 0
		end
		char.aiming = false
	end

	if(btnp(4, 0)) then
		char.aiming = true
		if(char.shootframes == 0) then
			char_shoot()
		end
	elseif(btnp(5, 0)) then
		--create_underground_enemy()
		pick_up_battery()
		jump_in()
	end

	if(current_t - char.inv_timer >= char.inv_time) then
		char.invulnerable = false
	end
	
	char_gravity()
	char_walls()
	char_top()
	char_spikes()
	char.x = char.x + char.speed.x
	char.y = char.y + char.speed.y

	if(char.hp <= 0) then
		ending = true
		end_text = "you have died."
	end

	if(get_flag(char.x+4, char.y+8, 7)) then
		win = true
	end
end

function bullet_update()
	--bullet update
	for bullet in all(bullets) do
		if(bullet.dir) then
			bullet.x = bullet.x - 2
		else
			bullet.x = bullet.x + 2
		end

		-- delete bullets that are not visible
		if(bullet.x < cam.x or bullet.x > (cam.x + 128)) then
			del(bullets, bullet)
		end

		local wflag = get_flag(bullet.x+6, bullet.y+1, 0)
		if(wflag) then
			del(bullets, bullet)
		end
		local wflag = get_flag(bullet.x+2, bullet.y+1, 0)
		if(wflag) then
			del(bullets, bullet)
		end
	end
end

function other_update()
	--enemy update
	for enemy in all(enemies) do
		-- move enemies if they are underground (always move) or visible
		if(enemy.underground or (enemy.x > cam.x and enemy.x < cam.x+128 and enemy.y > cam.y+28 and enemy.y < cam.y+128)) then
			if(enemy.underground) then
				if(not enemy.dir) then
					-- check for opposing wall
					local lflag = get_flag(enemy.x-1, enemy.y+2, 0)

					if(not lflag) then
						enemy.x = enemy.x - 0.5
					else
						enemy.dir = true
					end

					-- check if there's ledge
					lflag = get_flag(enemy.x-1, enemy.y+9, 0)

					if(not lflag) then
						enemy.dir = true
					else
						enemy.x = enemy.x - 0.5
					end
				else
					local rflag = get_flag(enemy.x+6, enemy.y+2, 0)

					if(not rflag) then
						enemy.x = enemy.x + 0.5
					else
						enemy.dir = false
					end

					rflag = get_flag(enemy.x+6, enemy.y+9, 0)

					if(not rflag) then
						enemy.dir = false
					else
						enemy.x = enemy.x + 0.5
					end
				end
				--todo: detect collisions
			elseif(og_moving) then
				if(char.inside) then
					enemy.x = enemy.x - 0.5
				else
					enemy.x = enemy.x - 0.75
				end
			end
		end

		-- char collision
		-- todo: upper head doesn't seem to collide?
		if(not char.inside) then
			if(char.x+4 > enemy.x and char.x+4 < enemy.x+8) then
				if((enemy.y > char.y and enemy.y < char.y+16) or (enemy.y + enemy.height > char.y and enemy.y + enemy.height < char.y+16)) then
					char_take_damage(enemy)
				end
			end
		end

		-- mech collision
		if(mech.sprx+4 > enemy.x and mech.sprx+4 < enemy.x+8) then
			if((enemy.y > mech.spry and enemy.y < mech.spry+mech.height) or (enemy.y + enemy.height > mech.spry and enemy.y + enemy.height < mech.spry+mech.height)) then
				mech_take_damage()
			end
		end


		-- checking bullet collisions here
		-- probably less bullets than enemies
		for bullet in all(bullets) do
			if(bullet.x+6 > enemy.x+2 and bullet.x+5 < (enemy.x+8)) then
				if(bullet.y+1 > enemy.y and bullet.y+1 < enemy.y+8*enemy.height) then
					if(enemy.health <= bullet.damage) then
						del(enemies, enemy)
					else
						enemy.health = enemy.health - bullet.damage
					end
					
					del(bullets, bullet)
					sfx(2)
				end
			end
		end

		for rocket in all(rockets) do
			if(rocket.explode) then
				local rx = rocket.x + rocket.dx
				local ry = rocket.y + rocket.dy
				if(rx-12 < enemy.x+4 and rx+12 > enemy.x+4) then
					if(ry-17 < enemy.y+4 and ry+5 > enemy.y+4) then
						del(enemies, enemy)
					end
				end
			end
		end
	end

	for rocket in all(rockets) do
		if(rocket.explode) then
			del(rockets, rocket)
			sfx(5)
		end
	end

	-- set overgound enemy moving flag. always at height = 3
	if(get_flag(mech.sprx+4, 3*8, 6)) then
		og_moving = not og_moving
		mset((mech.sprx+4)/8, 3, 0)
	elseif(get_flag(char.x+4, 3*8, 6)) then
		og_moving = not og_moving
		mset((char.x+4)/8, 3, 0)
	end
end

function char_gravity()
	-- gravity doesn't work on ladders
	local ladder = get_flag(char.x+4, char.y+15, 1)
	if(ladder) then
		char.speed.y = 0
		return
	end

	-- gravity
	local flag = get_flag(char.x+4, flr(char.y)+16, 0)
	if(flag and char.speed.y >= 0) then
		char.speed.y = 0
		char.y = flr((char.y+8)/8)*8-8
		char.jump = false
	else
		char.speed.y = char.speed.y + 0.5
		if(char.speed.y > 4) then char.speed.y = 4 end
	end
end

function char_up()
	local ladder = get_flag(char.x+4, char.y+15, 1)
	
	if(ladder) then
		char.y = char.y - 1
	elseif(not char.jump) then
		char.speed.y = -5
		char.jump = true
		sfx(0, 0)
	end
end

function char_down()
	local ladder = get_flag(char.x+4, char.y+15, 1)

	if(ladder) then
		char.y = char.y + 1
	else
		char.aiming = true
	end
end

function char_walls()
	local x = char.x
	-- char x speed works as a direction
	-- as it's always either -1 or 1
	local dir = 4+char.speed.x*4
	local flag = get_flag(x+dir, char.y+8, 0)
	
	if(flag) then
		char.speed.x = 0
	end
end

function char_top()
	local flag = get_flag(char.x+4, flr(char.y+4), 0)
	
	if(flag and char.jump) then
		char.speed.y = 1
	end
end

function char_spikes()
	local flag = get_flag(char.x+4, char.y+12, 3)

	if(flag) then
		char_take_damage()
	end
end

function char_take_damage(enemy)
	if(not char.invulnerable) then
		if(enemy == nil or enemy.underground) then
			char.hp = char.hp - 1
		else
			char.hp = char.hp - 2
		end
		sfx(3)
		set_inv(char)
	end
end

function mech_take_damage()
	if(not mech.invulnerable) then
		mech.hull = mech.hull - 2
		sfx(3)
		set_inv(mech)
	end
end

function set_inv(obj)
	obj.invulnerable = true
	obj.inv_timer = time()
end

function char_shoot()
	add(bullets, {x = char.x, y = char.y+8, dir = char.flipx, damage = 1})
	char.shootframes = 6
	sfx(1)
end

function mech_shoot()
	add(bullets, {x = mech.sprx, y = mech.spry+7, dir = false, damage = 4})
	sfx(1)
	mech.battery = mech.battery - 1
	mech.shooting = true
end

function mech_rockets()
	add(rockets, {x = mech.sprx, y = mech.spry+1, dx = mech.sprx + 100, dy = mech.spry-mech.height, step = 1, explode = false, damage = 8})
	sfx(4)
	mech.battery = mech.battery - 2
	mech.shooting = true
end

function rocket_path(rocket)
	paths = {
		{x = 0, y = 0, dx = 0, dy = -3},
		{x = 1, y = -5, dx = 3, dy = -7},
		{x = 4, y = -9, dx = 7, dy = -11},
		{x = 9, y = -10, dx = 13, dy = -10},
		{x = 18, y = -9, dx = 22, dy = -8},
		{x = 22, y = -8, dx = 27, dy = -7},
		{x = 30, y = -6, dx = 33, dy = -5},
		{x = 36, y = -4, dx = 39, dy = -2},
		{x = 42, y = -1, dx = 45, dy = 0},
		{x = 48, y = 1, dx = 51, dy = 2},
		{x = 52, y = 4, dx = 54, dy = 6},
		{x = 55, y = 8, dx = 57, dy = 10},
		{x = 58, y = 12, dx = 60, dy = 14},
		{x = 61, y = 17, dx = 63, dy = 20}
	}
	if(rocket.step > #paths) then
		rocket.dx = paths[#paths].dx
		rocket.dy = paths[#paths].dy
		rocket.explode = true
		local rx = rocket.x + rocket.dx
		local ry = rocket.y + rocket.dy
		return {x = 200, y = 200, dx = 200, dy = 200}
	end

	local coordinates = paths[rocket.step]
	rocket.step = rocket.step + 1
	return coordinates
end

function pick_up_battery()
	if(mech.battery >= 10) then return end
	if(mech.battery < 0) then mech.battery = 0  end -- bugged somewhere?
	
	for battery in all(batteries) do
		if(char.x+2 > battery.x and char.x+6 < battery.x+8) then
			if(char.y+10 > battery.y and char.y+14 < battery.x+8) then
				del(batteries, battery)
				mech.battery = mech.battery + 3.5
				sfx(6)
				if(mech.battery > 10) then mech.battery = 10 end
			end
		end
	end
end

function jump_in()
	if(mech.battery < 10) then return end

	if(char.x+4 > mech.sprx and char.x+4 < mech.sprx+8) then
		if(char.y > mech.spry and char.y < mech.spry+16) then
			char.inside = true
			sfx(9)
		end
	end
end

function create_ground_enemy()
	local enemy = {x = 10+char.x, y = char.y, dir = false, frame = 0, maxframes = 3, dir = false, height = 2, animated = true, sprite = 74, underground = false}
	add(enemies, enemy)
end

function create_underground_enemy()
	local enemy = {x = 10+char.x, y = char.y+8, dir = false, frame = 0, maxframes = 3, dir = false, height = 1, animated = true, sprite = 106, underground = true}
	add(enemies, enemy)
end

